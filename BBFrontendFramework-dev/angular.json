{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "auth": {
      "projectType": "library",
      "root": "projects/auth",
      "sourceRoot": "projects/auth/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/auth/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/auth/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/auth/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
          
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "tsConfig": "projects/auth/tsconfig.spec.json",
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ]
          }
        }
      }
    },
    "realServ-ui": {
      "projectType": "application",
      "schematics": {},
      "root": "projects/realServ-ui",
      "sourceRoot": "projects/realServ-ui/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/realServ-ui",
            "index": "projects/realServ-ui/src/index.html",
            "main": "projects/realServ-ui/src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "projects/realServ-ui/tsconfig.app.json",
            "assets": [
              "projects/realServ-ui/src/favicon.ico",
              "projects/realServ-ui/src/assets"
            ],
            "styles": [
              "projects/realServ-ui/src/styles.scss",
              "node_modules/summernote/dist/summernote-lite.min.css",
              "node_modules/summernote/dist/summernote.css",
              "node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
              "node_modules/@odymaui/angular-tree-component/css/angular-tree-component.css"
            ],
            "scripts": [
              "./node_modules/bootstrap/dist/js/bootstrap.min.js",
              "./node_modules/jquery/dist/jquery.min.js",
              "node_modules/summernote/dist/summernote.js",
              "node_modules/summernote/dist/summernote-bs4.min.js",
              "node_modules/summernote/dist/summernote-lite.min.js"  
            ]   
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "10mb",
                  "maximumError": "10mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "10mb",
                  "maximumError": "10mb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "projects/realServ-ui/src/environments/environment.ts",
                  "with": "projects/realServ-ui/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "staging": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "10mb",
                  "maximumError": "10mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "10mb",
                  "maximumError": "10mb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "projects/realServ-ui/src/environments/environment.ts",
                  "with": "projects/realServ-ui/src/environments/environment.stage.ts"
                }
              ],
              "outputHashing": "all"
            },
            "qa": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "10mb",
                  "maximumError": "10mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "10mb",
                  "maximumError": "10mb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "projects/realServ-ui/src/environments/environment.ts",
                  "with": "projects/realServ-ui/src/environments/environment.qa.ts"
                }
              ],
              "outputHashing": "all"
            },
            "dev": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "10mb",
                  "maximumError": "10mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "10mb",
                  "maximumError": "10mb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "projects/realServ-ui/src/environments/environment.ts",
                  "with": "projects/realServ-ui/src/environments/environment.dev.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "realServ-ui:build:production"
            },
            "staging": {
              "browserTarget": "realServ-ui:build:staging"
            },
            "qa": {
              "browserTarget": "realServ-ui:build:qa"
            },
            "dev": {
              "browserTarget": "realServ-ui:build:dev"
            },
            "development": {
              "browserTarget": "realServ-ui:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "realServ-ui:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "projects/realServ-ui/tsconfig.spec.json",
            "assets": [
              "projects/realServ-ui/src/favicon.ico",
              "projects/realServ-ui/src/assets"
            ],
            "styles": [
              "projects/realServ-ui/src/styles.scss",
              "node_modules/summernote/dist/summernote-lite.min.css"
            ],
            "scripts": [
              "./node_modules/bootstrap/dist/js/bootstrap.min.js",
              "./node_modules/jquery/dist/jquery.min.js",
              "node_modules/summernote/dist/summernote-lite.min.js" 
            ]

          }
        }
      }
    },
    "emr-ui": {
